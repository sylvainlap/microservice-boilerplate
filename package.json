{
  "name": "microservice-boilerplate",
  "version": "0.0.1",
  "description": "ECMAScript 2018 rest boilerplate with express",
  "author": "@sylvainlap",
  "main": "src/index.js",
  "engines": {
    "node": "8.12.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "dev": "nodemon --inspect ./src/index.js",
    "docs": "apidoc -i src/ -o docs/",
    "lint": "eslint ./src/",
    "precommit": "npm run lint",
    "prestart": "npm run docs",
    "start": "node ./src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sylvainlap/microservice-boilerplate.git"
  },
  "keywords": [
    "microservice",
    "rest",
    "boilerplate",
    "express"
  ],
  "dependencies": {
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "helmet": "^3.13.0",
    "knex": "^0.15.2",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.4.3"
  },
  "devDependencies": {
    "apidoc": "^0.17.6",
    "eslint": "^5.5.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "nodemon": "^1.18.4"
  },
  "license": "MIT"
}
